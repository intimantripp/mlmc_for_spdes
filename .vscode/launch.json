{
    "version": "0.2.0",
    "configurations":[
        {
            "name": "Debug MLMC",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/mlmc_cpp/build/mlmc_cpp",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/mlmc_cpp",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "variables": {
                "autoExpand": false
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
        }
    ]
}
